<!--物品信息-->
<view class="wupin">
	<view class="box" bindtap="details" wx:for="{{all}}" id="{{item._id}}">
		<image src="{{item.img}}" />
		<view class="neirong">
			<text class="text1">{{item.name}}</text>
			<text class="text2">{{item.describle}}</text>
			<view class="img">
				<image src="/images/money.png" /><text class="text3">{{item.price}}.00元</text>
				<image src="/images/xiaoxi.png" /><text class="text3">点击联系</text>
			</view>
			<text class="text4"></text>
		</view>
	</view>
</view>